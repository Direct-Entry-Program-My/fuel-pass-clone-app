<AnchorPane fx:id="pneWelcome" prefHeight="319.0" prefWidth="600.0" stylesheets="@../css/welcome-form.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WelcomeFormController">
